{
  "includeguard": {
    "prefix": ["includeguard"],
    "body": [
        "#ifndef $TM_INCLUDEGUARD",
        "#define $TM_INCLUDEGUARD",
        "",
        "",
        "#endif // $TM_INCLUDEGUARD"
    ]
  },
  "bmwcopyright": {
    "prefix": ["bmwcopyright"],
    "body": [
        "///",
        "/// @file",
        "/// @copyright Copyright (C) $CURRENT_YEAR, Bayerische Motoren Werke Aktiengesellschaft (BMW AG)",
        "///"
    ]
  },
  "debugprint": {
    "prefix": ["std::cerr", "debugprint"],
    "body": [
        "std::cerr << __PRETTY_FUNCTION__ << \" $1: \" << $2 << '\\n';"
    ]
  }
}

